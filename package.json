{
  "name": "kakao-message-client",
  "private": true,
  "version": "0.0.0",
  "description": "Kakao Message Application",
  "author": "Jewook Park <lukas.park.dev@gmail.com>",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run react-dev\" \"npm run electron-dev\"",
    "react-dev": "vite",
    "electron-dev": "cross-env NODE_ENV=development wait-on http://localhost:5173 && electron electron/main.js",
    "build": "cross-env NODE_ENV=production vite build",
    "start": "npm run dev",
    "preview": "vite preview",
    "lint": "eslint .",
    "preelectron-pack": "npm run build && electron-builder",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.2.1",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "http-status-codes": "^2.3.0",
    "jose": "^5.9.6",
    "jwt-decode": "^4.0.0",
    "qs": "^6.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.1",
    "recoil": "^0.7.7",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.17",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/redux": "^3.6.31",
    "@types/redux-persist": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.5",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.yourapp.kakao-message-client",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "electron/main.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    },
    "asar": true
  }
}
